generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CampCheckin {
  id        String   @id @default(uuid())
  name      String
  lpUrl     String?  @map("lp_url")
  comment   String?
  createdAt DateTime @default(now()) @map("created_at")

  @@map("camp_checkins")
}
